local MainSection = Tab:CreateSection("Farming")

Rayfield:Notify({
   Title = "You have executed Gojo Hub",
   Content = "Enjoy!",
   Duration = 5,
   Image = 9242918232,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})
